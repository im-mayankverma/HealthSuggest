  {
    "model": "symptoms.condition",
    "pk": 1,
    "fields": {
      "name": "Flu (Influenza)",
      "description": "A viral infection that attacks your respiratory system (nose, throat, and lungs).",
      "severity": "moderate",
      "advice": "Rest, drink plenty of fluids, and use fever-reducing medication if needed.",
      "home_remedies": "Stay hydrated, use warm fluids, get plenty of rest, and use a humidifier if available.",
      "when_to_see_doctor": "If symptoms last more than 3 days, if you have difficulty breathing, chest pain, or confusion."
    }
  },
  {
    "model": "symptoms.condition",
    "pk": 2,
    "fields": {
      "name": "Common cold",
      "description": "A mild viral infection of the upper respiratory tract (nose and throat).",
      "severity": "mild",
      "advice": "Rest and drink fluids. Over-the-counter medicine may help with symptoms.",
      "home_remedies": "Steam inhalation, warm drinks like soup or tea, and rest.",
      "when_to_see_doctor": "If symptoms last more than 10 days, if you have high fever, or if symptoms suddenly worsen."
    }
  },
  {
    "model": "symptoms.condition",
    "pk": 3,
    "fields": {
      "name": "COVID-like viral illness",
      "description": "A respiratory illness with symptoms that may be similar to COVID-19 or other viral infections.",
      "severity": "moderate",
      "advice": "Isolate from others, wear a mask around people, and rest. Seek testing if recommended locally.",
      "home_remedies": "Hydration, rest, and monitoring for worsening symptoms.",
      "when_to_see_doctor": "If you have trouble breathing, chest pain, confusion, or bluish lips/face."
    }
  },
  {
    "model": "symptoms.condition",
    "pk": 4,
    "fields": {
      "name": "Allergic rhinitis (hay fever)",
      "description": "An allergic reaction that causes sneezing, runny or stuffy nose, and itchy eyes.",
      "severity": "mild",
      "advice": "Avoid known triggers, keep windows closed during high pollen days, and consider antihistamines.",
      "home_remedies": "Saline nasal irrigation, showering after being outdoors, and using air filters.",
      "when_to_see_doctor": "If symptoms interfere with daily life or do not improve with basic treatment."
    }
  },
  {
    "model": "symptoms.condition",
    "pk": 5,
    "fields": {
      "name": "Acute bronchitis",
      "description": "Inflammation of the bronchial tubes, often after a viral infection, causing cough and chest discomfort.",
      "severity": "moderate",
      "advice": "Rest, drink fluids, and avoid smoking or smoke exposure.",
      "home_remedies": "Warm drinks, honey (for adults), and humidified air.",
      "when_to_see_doctor": "If cough lasts more than 3 weeks, you cough up blood, or have high fever or breathing difficulty."
    }
  },
  {
    "model": "symptoms.condition",
    "pk": 6,
    "fields": {
      "name": "Pneumonia (suspected)",
      "description": "An infection that inflames air sacs in one or both lungs, which may fill with fluid.",
      "severity": "severe",
      "advice": "Seek medical evaluation promptly. Pneumonia often needs antibiotics or hospital care.",
      "home_remedies": "Rest and hydration can support recovery but do not replace medical treatment.",
      "when_to_see_doctor": "Immediately, especially with shortness of breath, chest pain, high fever, or confusion."
    }
  },
  {
    "model": "symptoms.condition",
    "pk": 7,
    "fields": {
      "name": "Gastroenteritis (stomach flu)",
      "description": "An infection causing inflammation of the stomach and intestines, leading to diarrhea, vomiting and cramps.",
      "severity": "moderate",
      "advice": "Drink plenty of fluids to avoid dehydration and rest the gut with light foods.",
      "home_remedies": "Oral rehydration solutions, clear fluids, and bland foods like rice or toast when tolerated.",
      "when_to_see_doctor": "If you cannot keep fluids down, have blood in stool, or signs of severe dehydration."
    }
  },
  {
    "model": "symptoms.condition",
    "pk": 8,
    "fields": {
      "name": "Irritable bowel-like symptoms",
      "description": "Recurring stomach pain with changes in bowel habits without obvious infection.",
      "severity": "mild",
      "advice": "Track which foods trigger symptoms and manage stress levels.",
      "home_remedies": "Fiber adjustment, warm compress on the abdomen, and gentle physical activity.",
      "when_to_see_doctor": "If symptoms are persistent, severe, or associated with weight loss or blood in stool."
    }
  },
  {
    "model": "symptoms.condition",
    "pk": 9,
    "fields": {
      "name": "Urinary tract infection (UTI)",
      "description": "An infection in any part of the urinary system, often causing burning and frequent urination.",
      "severity": "moderate",
      "advice": "Increase fluid intake and seek medical advice for possible antibiotics.",
      "home_remedies": "Drinking water regularly and avoiding irritants like caffeine or alcohol.",
      "when_to_see_doctor": "If you have fever, back pain, blood in urine, or symptoms do not improve in 1\u20132 days."
    }
  },
  {
    "model": "symptoms.condition",
    "pk": 10,
    "fields": {
      "name": "Tension-type headache",
      "description": "A common headache with a band-like pressure around the head.",
      "severity": "mild",
      "advice": "Take breaks from screens, manage stress, and consider over-the-counter pain relief if appropriate.",
      "home_remedies": "Resting in a quiet, dark room, stretching neck muscles, and staying hydrated.",
      "when_to_see_doctor": "If headaches are frequent, sudden and severe, or associated with neurological symptoms."
    }
  },
  {
    "model": "symptoms.condition",
    "pk": 11,
    "fields": {
      "name": "Migraine-like headache",
      "description": "A throbbing headache often on one side, sometimes with nausea and light sensitivity.",
      "severity": "moderate",
      "advice": "Rest in a quiet, dark room and avoid known triggers.",
      "home_remedies": "Cold or warm compress on the head/neck and controlled breathing techniques.",
      "when_to_see_doctor": "If headaches are new, very severe, or different from your usual pattern."
    }
  },
  {
    "model": "symptoms.condition",
    "pk": 12,
    "fields": {
      "name": "Anxiety-related symptoms",
      "description": "Physical symptoms such as palpitations, sweating, and restlessness related to anxiety.",
      "severity": "mild",
      "advice": "Practice relaxation techniques and ensure regular sleep and exercise.",
      "home_remedies": "Deep breathing, mindfulness exercises, and talking with supportive people.",
      "when_to_see_doctor": "If anxiety interferes with daily life or you have thoughts of self-harm."
    }
  },
  {
    "model": "symptoms.condition",
    "pk": 13,
    "fields": {
      "name": "Depression-like symptoms",
      "description": "Low mood, reduced interest, poor sleep, or low energy for a prolonged period.",
      "severity": "moderate",
      "advice": "Reach out for support from trusted people and consider speaking to a professional.",
      "home_remedies": "Gentle activity, regular routine, and doing small enjoyable activities.",
      "when_to_see_doctor": "If symptoms last more than two weeks or you have any thoughts of self-harm."
    }
  },
